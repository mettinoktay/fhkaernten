clc
clear 

N = [1 2 5 10 100 1000];

dataset = zeros(1, length(N));
dataset_mean = zeros(1, length(N));
dataset_std = zeros(1, length(N));

index = 1;
for ii = N
    disp(ii)
    dataset = randi([1, 2], 1, ii);
    dataset = 2 * dataset - 3;
    
    dataset_mean(index) = mean(dataset);
    dataset_std(index) = std(dataset);
    index = index + 1;
end
